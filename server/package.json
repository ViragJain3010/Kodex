{
  "name": "server",
  "version": "1.0.0",
  "description": "Code execution server for Kodex",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node index.js",
    "lint": "pnpm format:check && pnpm lint:check",
    "lint:check": "eslint --quiet",
    "lint:fix": "eslint --fix --quiet",
    "format": "prettier --write \"**/*.{js,ts,json}\"",
    "format:check": "prettier --check",
    "test": "turbo run vitest -- run",
    "test:watch": "turbo vitest",
    "test:ui": "turbo vitest -- --ui",
    "test:coverage": "turbo vitest -- --coverage run",
    "test:ci": "turbo run vitest -- run",
    "vitest": "vitest"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dockerode": "^3.3.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.0.3",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "prisma": "^6.0.1",
    "supertest": "^7.1.0",
    "vitest": "^3.1.1"
  }
}
