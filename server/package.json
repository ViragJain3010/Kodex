{
  "name": "server",
  "version": "1.0.0",
  "description": "Code execution server for Kodex",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "lint": "eslint --fix --quiet && prettier --write \"**/*.{js,ts,json}\"",
    "lint:check": "eslint --quiet && prettier --check \"**/*.{js,ts,json}\"",
    "test": "turbo run vitest -- run",
    "test:watch": "turbo vitest",
    "test:ui": "turbo vitest -- --ui",
    "test:coverage": "turbo vitest -- --coverage run",
    "test:ci": "turbo run vitest -- run",
    "vitest": "vitest"
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dockerode": "^4.0.9",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^9.0.3",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "globals": "^17.1.0",
    "prisma": "^7.3.0",
    "supertest": "^7.2.2",
    "vitest": "^4.0.18"
  }
}
